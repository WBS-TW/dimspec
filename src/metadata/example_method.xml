<method>
  <chromatography>
    <ctype>LC</ctype>
    <vendor>Dionex</vendor>
    <mobilephase>
      <mp order=1>
        <solvent order=1>water</solvent>
        <fraction order=1>0.9</fraction>
        <solvent order=2>acetonitrile</solvent>
        <fraction order=2>0.1</fraction>
        <additive>10 mM ammonium acetate</additive>
      </mp>
      <mp order=2>
        <solvent order=1>acetonitrile</solvent>
        <fraction order=1>0.9</fraction>
        <solvent order=2>water</solvent>
        <fraction order=2>0.1</fraction>
        <additive>10 mM ammonium acetate</additive>
      </mp>
      <gradientprogram>
        <ratio time=0 time_unit = "min" value=2>0.1</ratio>
        <ratio time=15 time_unit = "min" value=2>0.9</ratio>
        <ratio time=16 time_unit = "min" value=2>1.0</ratio>
        <ratio time=16.1 time_unit = "min" value=2>0.1</ratio>
      </gradientprogram>
    </mobilephase>
    <column>
      <name>Eclipse Plus C18</name>
      <vendor>Agilent Technologies</vendor>
      <chemistry type="RP">C18</chemistry>
      <id units="mm">2.1</id>
      <length units="mm">50</length>
      <dp units="um" type="pellicular">1.8</dp>
    </column>
  </chromatography>
  <massspectrometry>
    <vendor>ThermoFisher Scientific</vendor>
    <ionization>
      <mode>electrospray ionization</mode>
      <polarity>positive</polarity>
      <voltage units="V" type="voltage">4500</voltage>
    </ionization>
    <massanalyzer>
      <analyzer order=1>quadrupole</analyzer>
      <analyzer order=2>Orbitrap</analyzer>
    </massanalyzer>
    <fragmentation>
      <mode>beam-type collision-induced dissociation</mode>
      <collisionenergy units = "normalized" type = "NCE">45</collisionenergy>
    </fragmentation>
    <detector>inductive detector</detector>
  </massspectrometry>
  <qc_method>
    <used>TRUE</used>
    <qc_type>Instrument Calibration</qc_type>
    <qc_type>Internal Standard Validation</qc_type>
  </qc_method>
</method>